{
	"$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"types": ["./mod.d.ts"],
		"jsxFactory": "window.createElement"
	},
	"lint": {
		"include": ["src/"],
		"exclude": ["src/old/"],
		"rules": {
			"tags": ["recommended"],
			"include": ["eqeqeq", "no-await-in-loop"],
			"exclude": ["no-explicit-any"]
		}
	},
	"imports": {
		"bot": "./src/bot.ts",
		"log": "./src/log.ts",
		"jsx": "./src/jsx/mod.ts",
		"ddTypes": "./src/ddTypes.ts",
		"modules": "./src/modules/mod.ts",
		"axiod": "https://deno.land/x/axiod@0.26.2/mod.ts",
		"axiodInterfaces": "https://deno.land/x/axiod@0.26.2/interfaces.ts",
		"bytes": "https://deno.land/std@0.184.0/fmt/bytes.ts",
		"canvas": "https://deno.land/x/canvas@v1.4.1/mod.ts",
		"colorConvert": "https://deno.land/x/color@v0.3.0/mod.ts",
		"commonTags": "https://esm.sh/common-tags@1.8.2#=",
		"discordeno": "npm:@discordeno/bot@19.0.0-next.2b83d69",
		"dom": "https://deno.land/x/deno_dom@v0.1.38/deno-dom-wasm.ts",
		"duration": "https://deno.land/std@0.184.0/fmt/duration.ts",
		"exif": "https://deno.land/x/deno_exif@0.0.3/mod.ts",
		"fuse": "https://esm.sh/fuse.js@6.6.2",
		"genius": "npm:genius-lyrics-api",
		"googleTranslate": "https://deno.land/x/gtr@v0.0.1/mod.ts",
		"mathjs": "https://esm.sh/mathjs@11.8.0/",
		"otpauth": "https://deno.land/x/otpauth@v9.1.1/dist/otpauth.esm.js",
		"path": "https://deno.land/std@0.184.0/path/mod.ts",
		"progressbar": "https://esm.sh/string-progressbar@1.0.4",
		"puppeteer": "https://deno.land/x/puppeteer@16.2.0/mod.ts",
		"temporal": "https://esm.sh/@js-temporal/polyfill@0.4.3",
		"time": "https://deno.land/x/time.ts@v2.0.1/mod.ts",
		"urban": "https://deno.land/x/urbanary@1.0.2/mod.ts",
		"wikipedia": "https://deno.land/x/denowiki@v0.2.1/mod.ts"
	}
}
