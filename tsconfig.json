{
  "display": "Node 16",
  "$schema": "https://json.schemastore.org/tsconfig",

  "compilerOptions": {
    "lib": ["es2021"],
    "target": "es2021",
    "module": "commonjs",
    "typeRoots": ["./node_modules/@types", "./Typings"],

    // "noEmit": true,
    "outDir": "./dist",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "alwaysStrict": false,
    "skipLibCheck": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "logs",
    "node_modules",
    "Old"
  ],
  "files": ["deps.ts", "mod.ts"],
  "include": ["Commands/**/*", "Events/*", "Modules/*", "Resources/**/*", "Old/**/*"]
}