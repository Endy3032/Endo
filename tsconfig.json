{
  "display": "Node 16",
  "$schema": "https://json.schemastore.org/tsconfig",

  "compilerOptions": {
    "lib": ["es2021"],
    "target": "es2021",
    "module": "ESNext",
    "typeRoots": ["./node_modules/@types", "typings"],

    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "alwaysStrict": false,
    "skipLibCheck": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "logs",
    "Resources",
    "node_modules"
  ]
}